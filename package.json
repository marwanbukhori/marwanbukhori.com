    "lucide-vue-next": "^0.474.0",
    "pinia": "^2.3.1",
    "radix-vue": "^1.9.13",
    "shadcn-vue": "^0.11.3",
    "tailwind-merge": "^2.6.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
